{
  "name": "datx-jsonapi",
  "version": "0.15.9",
  "description": "DatX mixin for JSON API support",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/datx.git"
  },
  "bugs": {
    "url": "https://github.com/infinum/datx/issues"
  },
  "homepage": "https://github.com/infinum/datx#readme",
  "author": "Infinum JavaScript Team <hello@infinum.co>",
  "license": "MIT",
  "keywords": [
    "datx",
    "mobx",
    "jsonapi"
  ],
  "devDependencies": {
    "@infinumjs/tslint-config": "^2.0.0-beta.6",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/nock": "^9.3.1",
    "@types/node": "^11.11.4",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.5.0",
    "lodash": "^4.17.11",
    "mobx": "^5.9.0",
    "nock": "^10.0.6",
    "rollup": "^1.6.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "mobx": "^4.2.0 || ^5.5.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "watch": "jest --watch --coverage",
    "lint": "tslint --project .",
    "prepublish": "npm run build",
    "build": "rollup -c"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/",
      "../datx/dist",
      "../datx-utils/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "test/main.ts",
    "transform": {
      ".ts": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  },
  "dependencies": {
    "datx": "^0.15.9",
    "datx-utils": "^0.15.9"
  }
}

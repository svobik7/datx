{
  "name": "datx",
  "version": "0.15.9",
  "description": "A MobX data store",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/datx.git"
  },
  "bugs": {
    "url": "https://github.com/infinum/datx/issues"
  },
  "homepage": "https://github.com/infinum/datx#readme",
  "author": "Infinum JavaScript Team <hello@infinum.co>",
  "license": "MIT",
  "devDependencies": {
    "@infinumjs/tslint-config": "^2.0.0-beta.6",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "jest": "^24.5.0",
    "mobx": "^5.9.0",
    "rollup": "^1.6.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "mobx": "^4.2.0 || ^5.5.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "watch": "jest --watch --coverage",
    "benchmark": "jest benchmark",
    "benchmark-watch": "jest benchmark --watch",
    "lint": "tslint --project .",
    "prepublish": "npm run build",
    "build": "rollup -c"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "test/(.*).ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "preset": "ts-jest",
    "testMatch": null
  },
  "dependencies": {
    "datx-utils": "^0.15.9"
  }
}
